<?php

namespace BackendBundle\Entity;

/**
 * TblBitacoraSecuencias
 */
class TblBitacoraSecuencias
{
    /**
     * @var integer
     */
    private $idBitacoraSecuencia;

    /**
     * @var string
     */
    private $codSecuencia;

    /**
     * @var integer
     */
    private $valor2Old;

    /**
     * @var integer
     */
    private $valor2New;

    /**
     * @var \BackendBundle\Entity\TblUsuarios
     */
    private $idUsuario;

    /**
     * @var \BackendBundle\Entity\TblDepartamentosFuncionales
     */
    private $idDeptoFuncional;


    /**
     * Get idBitacoraSecuencia
     *
     * @return integer
     */
    public function getIdBitacoraSecuencia()
    {
        return $this->idBitacoraSecuencia;
    }

    /**
     * Set codSecuencia
     *
     * @param string $codSecuencia
     *
     * @return TblBitacoraSecuencias
     */
    public function setCodSecuencia($codSecuencia)
    {
        $this->codSecuencia = $codSecuencia;

        return $this;
    }

    /**
     * Get codSecuencia
     *
     * @return string
     */
    public function getCodSecuencia()
    {
        return $this->codSecuencia;
    }

    /**
     * Set valor2Old
     *
     * @param integer $valor2Old
     *
     * @return TblBitacoraSecuencias
     */
    public function setValor2Old($valor2Old)
    {
        $this->valor2Old = $valor2Old;

        return $this;
    }

    /**
     * Get valor2Old
     *
     * @return integer
     */
    public function getValor2Old()
    {
        return $this->valor2Old;
    }

    /**
     * Set valor2New
     *
     * @param integer $valor2New
     *
     * @return TblBitacoraSecuencias
     */
    public function setValor2New($valor2New)
    {
        $this->valor2New = $valor2New;

        return $this;
    }

    /**
     * Get valor2New
     *
     * @return integer
     */
    public function getValor2New()
    {
        return $this->valor2New;
    }

    /**
     * Set idUsuario
     *
     * @param \BackendBundle\Entity\TblUsuarios $idUsuario
     *
     * @return TblBitacoraSecuencias
     */
    public function setIdUsuario(\BackendBundle\Entity\TblUsuarios $idUsuario = null)
    {
        $this->idUsuario = $idUsuario;

        return $this;
    }

    /**
     * Get idUsuario
     *
     * @return \BackendBundle\Entity\TblUsuarios
     */
    public function getIdUsuario()
    {
        return $this->idUsuario;
    }

    /**
     * Set idDeptoFuncional
     *
     * @param \BackendBundle\Entity\TblDepartamentosFuncionales $idDeptoFuncional
     *
     * @return TblBitacoraSecuencias
     */
    public function setIdDeptoFuncional(\BackendBundle\Entity\TblDepartamentosFuncionales $idDeptoFuncional = null)
    {
        $this->idDeptoFuncional = $idDeptoFuncional;

        return $this;
    }

    /**
     * Get idDeptoFuncional
     *
     * @return \BackendBundle\Entity\TblDepartamentosFuncionales
     */
    public function getIdDeptoFuncional()
    {
        return $this->idDeptoFuncional;
    }
    /**
     * @var boolean
     */
    private $actSecuencia = false;


    /**
     * Set actSecuencia
     *
     * @param boolean $actSecuencia
     *
     * @return TblBitacoraSecuencias
     */
    public function setActSecuencia($actSecuencia)
    {
        $this->actSecuencia = $actSecuencia;

        return $this;
    }

    /**
     * Get actSecuencia
     *
     * @return boolean
     */
    public function getActSecuencia()
    {
        return $this->actSecuencia;
    }
}
